@page "/complete-order-dialog"

<MudDialog>
    <DialogContent>
        The customer will now receive a notification letting them know to pick up their order.
    </DialogContent>
    <DialogActions>
        <MudButton Color="Color.Primary" OnClick="Submit" Href="/orders">Ok</MudButton>
    </DialogActions>
</MudDialog>


@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; }

    void Submit() => MudDialog.Close(DialogResult.Ok(true));
}